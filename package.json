    "react-native-modal-datetime-picker": "^6.0.0",
    "react-native-vector-icons": "^6.3.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.121",
    "@types/react": "^16.8.3",
    "@types/react-native": "^0.57.36",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "husky": "^1.3.1",
    "jest": "24.1.0",
    "lint-staged": "^8.1.4",
    "lodash": "^4.17.11",
    "metro-react-native-babel-preset": "0.51.1",
    "prettier": "^1.16.4",
